<h1>Listing location_properties</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @location_properties.each do |location_property| %>
  <tr>
    <td><%= location_property.id %></td>
    <td><%= location_property.name %></td>
    <td><%= location_property.description %></td>
    <td><%= link_to 'Show', location_property %></td>
    <td><%= link_to 'Edit', edit_location_property_path(location_property) %></td>
    <td><%= link_to 'Destroy', location_property, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

